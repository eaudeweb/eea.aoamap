<h1>Import Country Fiches from AoA portal</h1>

<form method="post" action=".import_countryfiches.submit"
      tal:define="aoa_data view/aoa_data">

  <label tal:repeat="country aoa_data/countries">
    <input type="checkbox" name="country:list"
           tal:attributes="value country"/>
    <tal:block content="country"/><br/>
  </label>

  <input type="submit"/>

</form>
