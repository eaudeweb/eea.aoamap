<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map>
<Map background-color="#9ab"
     srs="+proj=laea +lat_0=52 +lon_0=65">

  <Style name="Raster">
    <Rule>
      <RasterSymbolizer scaling="bilinear"/>
    </Rule>
  </Style>

  <Layer name="raster-gdal" srs="+proj=laea +lat_0=52 +lon_0=65">
    <StyleName>Raster</StyleName>
    <Datasource>
      <Parameter name="type">gdal</Parameter>
      <Parameter name="base">/Users/alexm/Desktop/aoa-tiled-map</Parameter>
      <Parameter name="file">cut-raster.tiff</Parameter>
    </Datasource>
  </Layer>

</Map>
